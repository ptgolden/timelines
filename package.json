{
  "name": "timelines",
  "version": "0.0.1",
  "author": "Patrick Golden",
  "description": "A Backbone application for creating and comparing timelines.",
  "private": true,
  "dependencies": {
    "backbone": "^1.1.2",
    "backbone-indexeddb": "superfeedr/indexeddb-backbonejs-adapter",
    "backbone.stickit": "^0.8.0",
    "d3": "^3.4.8",
    "jquery": "^1.11.0",
    "underscore": "^1.6.0"
  },
  "devDependencies": {
    "browserify": "^4.1.2",
    "browserify-shim": "^3.5.0",
    "node-underscorify": "0.0.9",
    "brfs": "1.0.0",
    "watchify": "^0.9.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "node-underscorify",
      "brfs"
    ]
  },
  "browser": {
    "d3-timeline": "./lib/d3-timeline.js",
    "jquery-bootstrap": "./lib/bootstrap.js"
  },
  "browserify-shim": {
    "d3-timeline": {
      "depends": [
        "d3"
      ]
    },
    "jquery-bootstrap": {
      "depends": [
        "jquery:jQuery"
      ]
    }
  }
}
